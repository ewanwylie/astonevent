uni.png'\n\n\nclass SignIn extends Component {\n  constructor() {\n    super();\n    this.state = {\n      email: \"\",\n      password: \"\",\n      form: []\n    };\n  }\n  handleChange = event => {\n    this.setState({\n      [event.target.name]: event.target.value\n    });\n  };\n  handleSubmit = event => {\n    const { email, password } = this.state;\n    //Data Validation\n    if (!email.includes(\"@\")) {\n      return alert(\"Please enter Valid email\");\n    }\n    //Creating JSON form to send to DB\n    this.setState({\n      form: [{ email, password }]\n    });\n    event.preventDefault();\n    //Post Request\n    fetch(`#`, {\n      method: \"POST\",\n      body: JSON.stringify(this.state.form),\n      headers: { \"Content-Type\": \"application/json\" }\n    }).then(res => {\n      //Clearing Form\n      this.setState({\n        email: \"\",\n        password: \"\"\n      });\n    });\n  };\n  render() {\n    return (\n      <section className=\"mainRegister\">\n        <div className=\"imgBlk\"><img src={imgSrc} /></div>\n\n        <h3>Sign In</h3>\n        <form onSubmit={this.handleSubmit} className=\"registerForm\">\n          <ul>\n            <li>\n              <label>\n\n                <input\n                  name=\"email\"\n                  type=\"text\"\n                  placeholder=\"Email Address\"\n                  onChange={this.handleChange} />\n              </label>\n            </li>\n            <li>\n              <label>\n\n                <input\n                  name=\"password\"\n                  type=\"password\"\n                  placeholder=\"Password\"\n                  onChange={this.handleChange} />\n              </label>\n            </li>\n          </ul>\n          <button className=\"btn\" type=\"submit\">Log In</button>\n        </form>\n      </section>\n    );\n  }\n}\nexport default SignIn;","C:\\Users\\ewylie\\git\\astonevent\\Aston Events Website\\react-front-end\\src\\components\\register\\Register.jsx"