{
	"name": "aston-events-website",
	"version": "1.0.0",
	"description": "Aston Events Website",
	"main": "server.js",
	"engines": {
		"node": "10.x",
		"npm": "6.x"
	},
	"repository": {
    "type": "git",
    "url": "https://github.com/ewanwylie/astonevent.git"
    },
	"scripts": {
		"start": "node server.js",
		"server": "nodemon server.js",
		"client": "npm start --prefix react-front-end",
