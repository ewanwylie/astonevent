		"dotenv": "^8.0.0",
		"express": "^4.16.4",
		"glob-parent": "^6.0.1",
		"gravatar": "^1.6.0",
		"jsonwebtoken": "^8.2.0",
		"mongodb": "^3.2.7",
		"mongoose": "^5.0.12",
		"morgan": "^1.9.0",
		"npm": "^6.10.0",
		"passport": "^0.4.0",
		"passport-jwt": "^4.0.0",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"react-responsive-carousel": "^3.1.49",
		"react-router-dom": "^5.0.1",
